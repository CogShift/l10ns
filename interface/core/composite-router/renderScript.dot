<!--

{
  contents : [
    {
      model : '',
      view : '',
      region : '',
      path : ''
    }
  ]
}

!-->

{{~it :content:index}}
    app.document.trigger('render{{=content.model.toLowerCase()}}start');
    try {
      document.querySelector('[data-region="{{=content.region}}"]').innerHTML = this.content['{{=content.region}}'];
    }
    catch(err) {
      throw err;
    }
    app.document.trigger('render{{=content.model.toLowerCase()}}end');
{{~}}
