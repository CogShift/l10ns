
<ul class="translations {{?!it.metas.revealed}}hidden{{?}}" data-content="translations">
  <li class="translations-headers">
    <h2 class="translation-col translations-header">{{=it.metas.l10n_keys}}</h2>
    <h2 class="translation-col translations-header">{{=it.metas.l10n_values}}</h2>
  <li>
  {{~it :translation:index}}
    <li class="translation" data-id="{{=translation.id}}">
      <div class="translation-col translation-key">{{=translation.key}}</div>
      <div class="translation-col translation-value">{{=translation.text}}</div>
    </li>
  {{~}}
</ul>
